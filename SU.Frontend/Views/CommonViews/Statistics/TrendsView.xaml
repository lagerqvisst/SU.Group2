<Window x:Class="SU.Frontend.Views.CommonViews.Statistics.TrendsView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="TrendsView" Height="450" Width="800">
    <Grid>
        <!-- TabControl för att hantera de tre olika sidorna -->
        <TabControl>
            <!-- Tab för tabellvyn -->
            <TabItem Header="Tabell">
                <Frame Source="Views/CommonViews/Statistics/TablePage.xaml"/>
            </TabItem>

            <!-- Tab för linjediagram -->
            <TabItem Header="Linjediagram">
                <Frame Source="Views/CommonViews/Statistics/LineChartPage.xaml"/>
            </TabItem>

            <!-- Tab för stapeldiagram -->
            <TabItem Header="Stapeldiagram">
                <Frame Source="Views/CommonViews/Statistics/BarChartPage.xaml"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
